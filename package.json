{
  "name": "@himenon/openapi-typescript-practice",
  "version": "0.0.0",
  "description": "OpenAPIを利用したAPI作成と、開発、リリースフローまでの仕組みづくりの練習",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Himenon/openapi-typescript-practice.git"
  },
  "license": "MIT",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/esm/index.d.ts",
  "scripts": {
    "build": "yarn clean && node ./tools/build.js && run-p tsc:build:* && run-s cherry-pick copy:lib",
    "cherry-pick": "cherry-pick --types-dir ./types --cjs-dir ./cjs --esm-dir ./esm --cwd ./lib --input-dir ../src",
    "clean": "rimraf lib dist build docs types yarn-error.log src",
    "copy:lib": "node ./tools/copy-lib.js",
    "mock:server": "node ./tools/server.js",
    "release:github:registry": "yarn publish --registry https://npm.pkg.github.com/:_authToken=${NODE_AUTH_TOKEN}",
    "release:npm:registry": "yarn publish",
    "tsc:build:cjs": "tsc -p tsconfig.cjs.json",
    "tsc:build:esm": "tsc -p tsconfig.esm.json",
    "tsc:build:types": "yarn tsc -d --emitDeclarationOnly --outDir lib/types"
  },
  "devDependencies": {
    "@apidevtools/swagger-cli": "4.0.4",
    "@babel/cli": "7.12.1",
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-export-default-from": "7.12.1",
    "@babel/plugin-transform-runtime": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-typescript": "7.12.1",
    "@babel/runtime": "7.12.1",
    "@openapi-generator-plus/typescript-fetch-client-generator": "0.5.3",
    "@openapitools/openapi-generator-cli": "2.0.3",
    "@stoplight/prism-cli": "4.1.0",
    "@types/node-fetch": "2.5.7",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "babel-plugin-add-module-exports": "1.0.4",
    "babel-plugin-dev-expression": "0.2.2",
    "chalk": "4.1.0",
    "cherry-pick": "0.5.0",
    "chokidar": "3.4.3",
    "conventional-changelog-angular-all": "1.7.0",
    "cpy": "8.1.1",
    "eslint": "7.12.0",
    "eslint-config-prettier": "6.14.0",
    "eslint-plugin-import": "2.22.1",
    "execa": "4.0.3",
    "husky": "4.3.0",
    "lerna": "3.22.1",
    "lint-staged": "10.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "redoc": "2.0.0-rc.40",
    "redoc-cli": "0.9.12",
    "rimraf": "3.0.2",
    "sort-package-json": "1.46.1",
    "typescript": "4.0.3"
  },
  "publishConfig": {
    "access": "public",
    "directory": "lib"
  }
}
